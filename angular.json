{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "angular-demos",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "angular-demos": {
      "projectType": "application",
      "root": "apps/angular-demos",
      "sourceRoot": "apps/angular-demos/src",
      "prefix": "nx-configuration",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular-demos",
            "index": "apps/angular-demos/src/index.html",
            "main": "apps/angular-demos/src/main.ts",
            "polyfills": "apps/angular-demos/src/polyfills.ts",
            "tsConfig": "apps/angular-demos/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/angular-demos/src/favicon.ico",
              "apps/angular-demos/src/assets"
            ],
            "styles": ["apps/angular-demos/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular-demos/src/environments/environment.ts",
                  "with": "apps/angular-demos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-demos:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-demos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-demos:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/angular-demos/src/**/*.ts",
              "apps/angular-demos/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/angular-demos"],
          "options": {
            "jestConfig": "apps/angular-demos/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "angular-demos-e2e": {
      "root": "apps/angular-demos-e2e",
      "sourceRoot": "apps/angular-demos-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-demos-e2e/cypress.json",
            "tsConfig": "apps/angular-demos-e2e/tsconfig.e2e.json",
            "devServerTarget": "angular-demos:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-demos:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/angular-demos-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "react-demos": {
      "root": "apps/react-demos",
      "sourceRoot": "apps/react-demos/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/react-demos",
            "index": "apps/react-demos/src/index.html",
            "main": "apps/react-demos/src/main.tsx",
            "polyfills": "apps/react-demos/src/polyfills.ts",
            "tsConfig": "apps/react-demos/tsconfig.app.json",
            "assets": [
              "apps/react-demos/src/favicon.ico",
              "apps/react-demos/src/assets"
            ],
            "styles": ["apps/react-demos/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-demos/src/environments/environment.ts",
                  "with": "apps/react-demos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-demos:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react-demos:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/react-demos/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/react-demos"],
          "options": {
            "jestConfig": "apps/react-demos/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-demos-e2e": {
      "root": "apps/react-demos-e2e",
      "sourceRoot": "apps/react-demos-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-demos-e2e/cypress.json",
            "tsConfig": "apps/react-demos-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-demos:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-demos:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/react-demos-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "angular": {
      "projectType": "library",
      "root": "libs/angular",
      "sourceRoot": "libs/angular/src",
      "prefix": "nx-configuration",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular/src/**/*.ts",
              "libs/angular/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/angular"],
          "options": {
            "jestConfig": "libs/angular/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/angular/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/angular",
            "config": {
              "configFolder": "libs/angular/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "react": {
      "root": "libs/react",
      "sourceRoot": "libs/react/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react"],
          "options": {
            "jestConfig": "libs/react/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/react/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/react",
            "config": {
              "configFolder": "libs/react/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "angular-e2e": {
      "root": "apps/angular-e2e",
      "sourceRoot": "apps/angular-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-e2e/cypress.json",
            "tsConfig": "apps/angular-e2e/tsconfig.e2e.json",
            "devServerTarget": "angular:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "angular:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/angular-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "react-e2e": {
      "root": "apps/react-e2e",
      "sourceRoot": "apps/react-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-e2e/cypress.json",
            "tsConfig": "apps/react-e2e/tsconfig.e2e.json",
            "devServerTarget": "react:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "react:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/react-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
