{
  "npmScope": "friday-design-systems",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "angular": {
      "tags": []
    },
    "angular-demos": {
      "tags": []
    },
    "angular-demos-e2e": {
      "tags": [],
      "implicitDependencies": [
        "angular-demos"
      ]
    },
    "angular-v2": {
      "tags": []
    },
    "angular-v2-demos": {
      "tags": []
    },
    "angular-v2-demos-e2e": {
      "tags": [],
      "implicitDependencies": [
        "angular-v2-demos"
      ]
    },
    "react": {
      "tags": []
    },
    "react-demos": {
      "tags": []
    },
    "react-demos-e2e": {
      "tags": [],
      "implicitDependencies": [
        "react-demos"
      ]
    }
  }
}