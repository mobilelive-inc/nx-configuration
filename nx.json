{
  "npmScope": "nx-configuration",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "angular-demos": {
      "tags": []
    },
    "angular-demos-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-demos"]
    },
    "react-demos": {
      "tags": []
    },
    "react-demos-e2e": {
      "tags": [],
      "implicitDependencies": ["react-demos"]
    }
  }
}
